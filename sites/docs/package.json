{
	"name": "docs",
	"version": "0.0.3",
	"private": true,
	"scripts": {
		"dev": "concurrently \"pnpm:dev:content\" \"pnpm:dev:svelte\"",
		"dev:content": "contentlayer dev",
		"dev:svelte": "vite dev",
		"build": "contentlayer build && vite build",
		"build:all": "pnpm -w build",
		"build:content": "contentlayer build",
		"build:content-cachebust": "contentlayer build --clearCache",
		"preview": "vite preview",
		"check": "pnpm build:content && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write . --cache",
		"sync": "svelte-kit sync"
	},
	"license": "MIT",
	"author": {
		"name": "Huntabyte",
		"url": "https://github.com/huntabyte"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/huntabyte/svecodocs.git"
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^4.7.2",
		"@sveltejs/kit": "^2.7.3",
		"@sveltejs/vite-plugin-svelte": "4.0.0-next.7",
		"@tailwindcss/typography": "^0.5.10",
		"@types/hast": "^3.0.4",
		"@types/mdast": "^4.0.3",
		"@types/node": "^22.7.4",
		"autoprefixer": "^10.4.17",
		"clsx": "^2.1.0",
		"concurrently": "^8.2.2",
		"contentlayer": "^0.3.4",
		"hast-util-to-html": "^9.0.0",
		"mdsx": "^0.0.6",
		"phosphor-svelte": "^2.0.1",
		"postcss": "^8.4.35",
		"postcss-load-config": "^5.0.3",
		"rehype-pretty-code": "^0.13.0",
		"rehype-slug": "^6.0.0",
		"remark-gfm": "^4.0.0",
		"shiki": "^1.1.3",
		"svelte": "5.0.0-next.260",
		"svelte-check": "4.0.3",
		"sveltekit-superforms": "^2.19.1",
		"tailwind-merge": "^2.2.1",
		"tailwind-variants": "^0.1.20",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.7.0",
		"typescript": "^5.6.2",
		"unified": "^11.0.4",
		"unist-builder": "^4.0.0",
		"unist-util-visit": "^5.0.0",
		"vite": "^5.4.10",
		"zod": "^3.23.8"
	},
	"type": "module",
	"dependencies": {
		"@melt-ui/svelte": "^0.76.2",
		"bits-ui": "1.0.0-next.12",
		"formsnap": "workspace:*",
		"lucide-svelte": "^0.447.0",
		"mode-watcher": "^0.4.1",
		"svelte-sonner": "^0.3.28",
		"vaul-svelte": "^0.3.2"
	},
	"engines": {
		"pnpm": ">=8.7.0",
		"node": ">=18"
	},
	"sideEffects": false,
	"packageManager": "pnpm@9.6.0"
}
